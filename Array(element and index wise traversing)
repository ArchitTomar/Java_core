class dd
{
public static void main (String gg[])
{
int arr[]={34,3,5,6};
System.out.println ("Index wise traversing");
for(int i=0;i<arr.length;i++)
{
System.out.println ("Index wise traversing is" + arr[i]);
}
System.out.println ("element wise traversing");
for(int i:arr)
{
System.out.println ("Index wise traversing is :" + i);
}
}
}
